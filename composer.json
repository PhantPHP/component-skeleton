{
    "name": "phant/component-skeleton",
    "description": "Start easily with component skeleton",
    "license": "MIT",
    "keywords": ["component skeleton", "composer component skeleton"],
    "authors": [
        {
            "name": "Lenny ROUANET",
            "email": "lennyrouanet@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "8.*"
    },
    "require-dev": {
        "phpstan/phpstan": "1.*",
        "phpunit/phpunit": "9.*"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse component --memory-limit=4G",
        "test": "vendor/bin/phpunit test --testdox"
    },
    "autoload": {
        "psr-4": {
            "Phant\\ComponentSkeleton\\": "component/"
        }
    }
}
